<?xml version="1.0" encoding="UTF-8"?><rss version="2.0"
	xmlns:content="http://purl.org/rss/1.0/modules/content/"
	xmlns:wfw="http://wellformedweb.org/CommentAPI/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:atom="http://www.w3.org/2005/Atom"
	xmlns:sy="http://purl.org/rss/1.0/modules/syndication/"
	xmlns:slash="http://purl.org/rss/1.0/modules/slash/"

	xmlns:georss="http://www.georss.org/georss"
	xmlns:geo="http://www.w3.org/2003/01/geo/wgs84_pos#"
	>

  <channel>
  	<title>Words Make Worlds</title>
  	<link>https://methodandmatter.com/words-make-worlds/</link>
  	<description>A publication about the art, science, and craft of the Coaching Leadership Style. Sent bi-weekly, Words Make Worlds delivers insight and practices to help increase leadership impact.</description>
  	<language>en-US</language>
  </channel>


	<% blog('words-make-worlds').articles.each do |article| %>
			<item>
				<pubDate><%= article.date.strftime('%B %Y') %></pubDate>
				<title><%= article.data.description %></title>
				<link><%= article.url %></link>

				<content><%= article.body %></content>
			</item>
	<% end %>

</rss>
